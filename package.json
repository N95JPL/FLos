{
  "name": "jagos",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "title": "JagOS",
  "description": "A Electron-React application for controlling a Jaguar XF System",
  "author": {
    "name": "N95JPL",
    "email": "joshua@jnpl.co.uk",
    "url": "http://jnpl.co.uk/"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.0",
    "color-convert": "^2.0.1",
    "electron-is-dev": "^2.0.0",
    "fast-xml-parser": "^4.0.12",
    "immer": "^9.0.16",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.5.0",
    "react-scripts": "^5.1.0-next.14",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.2.0",
    "socketcan": "^4.0.1",
    "tw-elements": "^1.0.0-alpha13",
    "xml2json": "^0.12.0",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "@eove/eslint-config-electron": "^1.8.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "ajv": "^8.11.2",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-builder": "^24.0.0-alpha.4",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "postcss": "^8.4.20",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.1",
    "tailwind-safelist-generator": "^1.0.0",
    "tailwindcss": "^3.2.4",
    "wait-on": "^7.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "npm run build && npm run electron-dev",
    "electron-dev": "BROWSER=none concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder build",
    "electron-build": "electron-rebuild -f -a x64 && electron-builder build --linux AppImage --x64",
    "preelectron-build": "npm run build",
    "preelectron-pack": "npm run build",
    "postinstall": "electron-rebuild -f",
    "reset": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f",
    "buildAll": "npm run linux64-build && npm run pi32-build && npm run pi64-build",
    "linux64-build": "electron-rebuild -f -a x64 && electron-builder build --linux deb --x64",
    "pi32-build": "electron-rebuild -f -a armv7l && electron-builder --linux AppImage --armv7l",
    "pi64-build": "electron-rebuild -f -a arm64 && electron-builder build --linux AppImage --arm64",
    "pretty": "prettier --write \"./**/*/*/*.{js,jsx,json,xml}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "productName": "JagOS",
    "npmRebuild": false,
    "artifactName": "JagOS-${version}-${arch}.${ext}",
    "appId": "io.jplogics.jagos",
    "files": [
      "./src/**/*",
      "./build/**/*"
    ],
    "extraResources": [
      {
        "from": "./public/resources/JSON/",
        "to": "JSON",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "app": "./",
      "output": "./dist",
      "buildResources": "./build"
    }
  }
}
